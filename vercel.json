{
  "git": {
    "deploymentEnabled": true
  },
  "headers": [
    {
      "source": "/manifest.json",
      "headers": [{ "key": "Content-Type", "value": "application/manifest+json" }]
    },
    {
      "source": "/favicon.svg",
      "headers": [{ "key": "Content-Type", "value": "image/svg+xml" }]
    },
    {
      "source": "/icons/(.*)",
      "headers": [{ "key": "Content-Type", "value": "image/png" }]
    }
  ]
}